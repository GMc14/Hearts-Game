  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pg-promise": "^8.5.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
